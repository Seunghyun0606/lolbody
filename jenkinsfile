pipeline {
    agent any
    
    stages {
        stage('Pull') {
            git branch: 'test_docker', url: 'https://lab.ssafy.com/s03-webmobile1-sub3/s03p13b105.git'
        }
        stage('Build') {
            steps {
                sh 'back/mongoApi/mvn clean install -DskipTests'
            }
        }
        stage('Test') {
            steps {
                
                sh 'back/mongoApi/mvn package'
                sh 'sudo docker run -i -t --name spring -p 8888:8888 springboot:0.1'

            }
        }
    }   
}